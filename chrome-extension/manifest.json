{
  "manifest_version": 2,
  "name": "JDTools",
  "description": "辅助京东网站操作",
  "version": "0.1",
  "browser_action": {
    "default_title": "JDTools",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://vip.m.jd.com/newPage/reward"
      ],
      "css": [
      ],
      "js": [
        "yaoyiyao.js"
      ]
    },
    {
      "matches": [
        "https://plus.m.jd.com/coupon/index/*"
      ],
      "css": [
      ],
      "js": [
        "coupon.js"
      ]
    }
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "permissions": [
    "storage",
    "alarms"
  ]
}
